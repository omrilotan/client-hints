{
  "name": "client-hints",
  "version": "0.2.1",
  "description": "🕵️‍♂️ Parse client hints headers",
  "keywords": [
    "client-hints",
    "headers",
    "🕵️‍♂️"
  ],
  "author": "omrilotan <hi@omrilotan.com> (https://omrilotan.com/)",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git@github.com:omrilotan/client-hints.git"
  },
  "files": [
    "index.*"
  ],
  "type": "module",
  "main": "index.cjs",
