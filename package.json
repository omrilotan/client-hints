    "lint": "eslint src/** --plugin log"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "esbuild": "^0.14.25",
    "eslint": "^7.18.0",
    "eslint-plugin-log": "^1.2.7",
    "mocha": "^9.2.1",
    "typescript": "^4.6.2"
  }
}
